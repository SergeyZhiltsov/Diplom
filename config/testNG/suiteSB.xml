<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Screen Builder regression" verbose="4" parallel="methods" thread-count="1">
    <!--Above line corresponds to running below 2 clasees in parallel threads-->
    <listeners>
        <listener class-name="com.acurian.selenium.listeners.AnnotationTransformer"/>
    </listeners>
    <test name="Screen Builder basic regression">
        <classes>
            <!--<class name="com.acurian.selenium.SB.SBMegaScreenerTest"/>-->
            <class name="com.acurian.selenium.SB.SBStandAloneTest"/>
        </classes>
    </test>
</suite>